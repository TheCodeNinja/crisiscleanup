<table>
  <thead>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Title</th>
      <th>Organization</th>
    </tr>
  </thead>
  <tbody>
      <tr>
        <td>
          	<%= contact.first_name %>
        </td>
        <td> 
        	<%= contact.last_name %> 
        </td>
        <td>
          	<%= contact.email %>
        </td>
        <td> 
        	Temporarily <a href="http://blog.crisiscleanup.org/2017/09/internal-phone-numbers-hidden.html" target="_blank">Hidden</a>
        </td>
        <td>
          	<%= contact.title %>
        </td>
        <td> 
        	<%= contact.legacy_organization.name if contact.legacy_organization %> 
        </td>
       
      </tr>
  </tbody>
</table>